#include<stio.h>
#include<conio.h>
int main()
{
  int a,b ; 
  a = 10 ; 
  b = 15 ; 
  printf("Values of variables before swapping are :"\n);
  print("Value of A -" "%d\n",&a);
  print("Value of B -" "%d\n",&b);
  a = a+b ; 
  b = a-b ; 
  a = a-b ;
  printf("Values of variables after swapping are :"\n);
  printf("Value of A -" "%d\n", &a);
  printf("Value of B -" "%d\n", &b);
  return(0);
 }
 
